<div class="box-file">
	<div class="hidden">
		<%= f.hidden_field field_name %>
	</div>

	<a class="btn btn-big">上传照片</a>
	<%= file_field_tag "file", data: {url: "http://up.qiniu.com", bucket: BUCKET_PICTURES, 'form-data'=>{token: qiniu_pictures_upload_token }}, class: "qiniu_picture_uploader" %>
</div>

<%= javascript_tag do %>
	qiniu_picture_uploader()
<% end %>
